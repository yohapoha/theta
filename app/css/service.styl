@import "colors.styl"
@import "simples.styl"

.service {
  width 100%
  margin-top 30px
  float left
}
.service-note {
  @extend .note
}
.service-note .service-note__title {
  @extend .note__title
}
.service-head {
  @extend .head
}
.service-head .service-head__title {
  @extend .title
}
.service-head .service-head__subtitle {
  @extend .subtitle
}
.service-body {
  @extend .body
}
.service-body .service-body__title {
  @extend .title
  padding-top 60px
  border-top 1px solid $line_color
}
.service-body .service-body__subtitle {
  @extend .subtitle
  margin-bottom 0
  border-bottom none
}